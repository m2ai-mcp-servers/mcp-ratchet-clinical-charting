# Ratchet MCP - Environment Configuration
# Copy this file to .env and fill in your values

# PointCare API Configuration
POINTCARE_API_URL=https://api.pointcare.com
POINTCARE_API_KEY=your-api-key-here

# OAuth Configuration (if applicable)
POINTCARE_CLIENT_ID=your-client-id
POINTCARE_CLIENT_SECRET=your-client-secret

# Supabase Configuration (for PointCare EMR Dashboard)
# Get these from your Supabase project: https://supabase.com/dashboard
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_ANON_KEY=your-anon-key-here

# Optional: Logging level
LOG_LEVEL=info
